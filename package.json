{
  "name": "pelias-placeholder",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "npm run units",
    "units": "node test/units.js | tap-spec",
    "funcs": "for case in test/cases/*.txt; do node test/case.js $case; done",
    "start": "node server/http.js",
    "extract": "bash cmd/wof_extract.sh > data/wof.extract",
    "build": "rm -f data/graph.json data/store.sqlite3; cat data/wof.extract | node cmd/load.js",
    "gentests": "cat data/wof.extract | node cmd/generate_tests.js > test/cases/generated.txt",
    "repl": "node cmd/repl.js",
    "cli": "node cmd/cli.js",
    "check-dependencies": "node_modules/.bin/npm-check --production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pelias/placeholder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pelias/placeholder/issues"
  },
  "homepage": "https://github.com/pelias/placeholder#readme",
  "dependencies": {
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "lower-case": "^1.1.4",
    "remove-accents": "^0.4.0",
    "sorted-intersect": "^0.1.4",
    "split2": "^2.1.1",
    "sqlite3": "^3.1.8",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "require-dir": "^0.3.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "pre-commit": [
    "check-dependencies"
  ]
}
